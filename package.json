{
  "name": "@nervill/metadelta",
  "version": "1.0.0",
  "description": "Salesforce CLI plugin to find recent metadata changes in an org",
  "type": "module",
  "main": "lib/index.js",
  "bin": {
    "metadelta": "bin/run"
  },
  "files": [
    "lib",
    "bin",
    "oclif.manifest.json"
  ],
  "scripts": {
    "clean": "rimraf lib oclif.manifest.json",
    "build": "npm run clean && oclif manifest && tsc",
    "prepack": "npm run build",
    "pack": "npm pack"
  },
  "keywords": [
    "salesforce",
    "sf",
    "plugin",
    "metadata"
  ],
  "author": "Nerio Villalobos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NerioVillalobos/plugin-metadelta.git"
  },
  "bugs": {
    "url": "https://github.com/NerioVillalobos/plugin-metadelta/issues"
  },
  "homepage": "https://github.com/NerioVillalobos/plugin-metadelta",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@oclif/core": "^3.15.1",
    "@salesforce/sf-plugins-core": "^3.0.0",
    "fast-xml-parser": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  },
  "oclif": {
    "bin": "metadelta",
    "commands": "./lib/commands"
  }
}
