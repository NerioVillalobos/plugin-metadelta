{
  "name": "@nervill/metadelta",
  "version": "1.0.17",
  "description": "Salesforce CLI plugin to find recent metadata changes in an org",
  "type": "module",
  "main": "lib/index.js",
  "exports": "./lib/index.js",
  "files": [
    "lib/",
    "messages/",
    "oclif.manifest.json",
    "README.md"
  ],
  "scripts": {
    "clean": "node -e \"import {rmSync} from 'node:fs';['lib','oclif.manifest.json'].forEach(p=>rmSync(p,{recursive:true,force:true}))\"",
    "build": "npm run clean && tsc",
    "postbuild": "node scripts/generate-manifest.mjs",
    "manifest": "node scripts/generate-manifest.mjs",
    "pack": "npm pack --ignore-scripts",
    "pack:tarball": "mkdir -p dist && npm pack --ignore-scripts --pack-destination dist && node scripts/create-tarball-alias.mjs"
  },
  "keywords": [
    "salesforce",
    "sf",
    "plugin",
    "metadata"
  ],
  "author": "Nerio Villalobos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NerioVillalobos/plugin-metadelta.git"
  },
  "bugs": {
    "url": "https://github.com/NerioVillalobos/plugin-metadelta/issues"
  },
  "homepage": "https://github.com/NerioVillalobos/plugin-metadelta",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "fast-xml-parser": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "typescript": "^5.6.3"
  },
  "oclif": {
    "bin": "sf",
    "commands": "./lib/commands",
    "dirname": "metadelta",
    "pluginType": "sf"
  }
}
